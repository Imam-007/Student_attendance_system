<!-- views/attendanceList.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Attendance List</title>
</head>
<body>
    <h1>Attendance List</h1>
    <ul>
        <% attendance.forEach(record => { %>
            <li><%= record.student_id %> - <%= record.date %></li>
        <% }); %>
    </ul>
</body>
</html>
